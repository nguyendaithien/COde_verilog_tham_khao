#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5604ccefa130 .scope module, "parity" "parity" 2 1;
 .timescale 0 0;
v0x5604ccf3f740_0 .var "A", 31 0;
v0x5604ccf3f840_0 .var "clock", 0 0;
v0x5604ccf3f900_0 .var "flag_parity", 0 0;
S_0x5604ccefa2b0 .scope task, "check_parity" "check_parity" 2 17, 2 17 0, S_0x5604ccefa130;
 .timescale 0 0;
v0x5604ccf0c9f0_0 .var "a", 31 0;
v0x5604ccf3f680_0 .var "flag", 0 0;
E_0x5604ccefaa60 .event posedge, v0x5604ccf3f840_0;
TD_parity.check_parity ;
    %pushi/vec4 3, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5604ccefaa60;
    %load/vec4 v0x5604ccf0c9f0_0;
    %xor/r;
    %store/vec4 v0x5604ccf3f680_0, 0, 1;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %end;
    .scope S_0x5604ccefa130;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5604ccf3f840_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5604ccefa130;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5604ccf3f840_0;
    %inv;
    %store/vec4 v0x5604ccf3f840_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5604ccefa130;
T_3 ;
    %pushi/vec4 45, 0, 32;
    %store/vec4 v0x5604ccf3f740_0, 0, 32;
    %end;
    .thread T_3;
    .scope S_0x5604ccefa130;
T_4 ;
    %wait E_0x5604ccefaa60;
    %load/vec4 v0x5604ccf3f740_0;
    %store/vec4 v0x5604ccf0c9f0_0, 0, 32;
    %fork TD_parity.check_parity, S_0x5604ccefa2b0;
    %join;
    %load/vec4 v0x5604ccf3f680_0;
    %store/vec4 v0x5604ccf3f900_0, 0, 1;
    %vpi_call 2 13 "$display", " flag = %b ", v0x5604ccf3f900_0 {0 0 0};
    %jmp T_4;
    .thread T_4;
    .scope S_0x5604ccefa130;
T_5 ;
    %delay 50, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "8.5.v";
