#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x56212dd95130 .scope module, "Block" "Block" 2 1;
 .timescale 0 0;
v0x56212dd952b0_0 .var *"_s0", 3 0; Local signal
v0x56212ddd9620_0 .var "a", 3 0;
v0x56212ddd9700_0 .var "b", 3 0;
v0x56212ddd97c0_0 .var "c", 3 0;
    .scope S_0x56212dd95130;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x56212ddd9620_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x56212ddd9700_0, 0, 4;
    %load/vec4 v0x56212ddd9620_0;
    %load/vec4 v0x56212ddd9700_0;
    %add;
    %store/vec4 v0x56212dd952b0_0, 0, 4;
    %pushi/vec4 20, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x56212dd952b0_0;
    %store/vec4 v0x56212ddd97c0_0, 0, 4;
    %vpi_call 2 8 "$monitor", $time, " c  = %b ", v0x56212ddd97c0_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x56212dd95130;
T_1 ;
    %vpi_call 2 11 "$monitor", $time, " c  = %b ", v0x56212ddd97c0_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
