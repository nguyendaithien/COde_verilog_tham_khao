    %store/vec4 v0x5648b1f09b80_0, 0, 2;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5648b1f09aa0_0, 0, 2;
    %jmp T_2.5;
T_2.4 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5648b1f09aa0_0, 0, 2;
    %jmp T_2.5;
T_2.5 ;
    %pop/vec4 1;
    %jmp T_2;
