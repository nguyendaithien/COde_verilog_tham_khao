    .scope S_0x5648b1ef3f90;
T_3 ;
    %vpi_call 2 8 "$dumpfile", "signal_control.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5648b1ef3f90 {0 0 0};
