    .thread T_3;
    .scope S_0x5648b1ef3f90;
T_4 ;
    %vpi_call 2 12 "$monitor", $time, " signal: Hight way: %d , Country way: %d", v0x5648b1f0a230_0, v0x5648b1f0a160_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5648b1ef3f90;
T_5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5648b1f0a0a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5648b1f0a330_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5648b1f0a400_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5648b1f0a330_0, 0, 1;
    %delay 10, 0;
