v0x5648b1f09b80_0 .var "hw", 1 0;
v0x5648b1f09c70_0 .var "next_state", 3 0;
v0x5648b1f09d50_0 .net "reset", 0 0, v0x5648b1f0a330_0;  1 drivers
v0x5648b1f09e60_0 .var "state", 3 0;
v0x5648b1f09f40_0 .net "x", 0 0, v0x5648b1f0a400_0;  1 drivers
E_0x5648b1eeeeb0 .event edge, v0x5648b1f09e60_0;
E_0x5648b1eee590 .event edge, v0x5648b1f09f40_0, v0x5648b1f09e60_0;
E_0x5648b1eeed60 .event posedge, v0x5648b1f09d50_0, v0x5648b1ee1080_0;
    .scope S_0x5648b1ef4110;
T_0 ;
    %wait E_0x5648b1eeed60;
    %load/vec4 v0x5648b1f09d50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5648b1f09e60_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5648b1f09e60_0;
    %assign/vec4 v0x5648b1f09c70_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5648b1ef4110;
T_1 ;
    %wait E_0x5648b1eee590;
    %load/vec4 v0x5648b1f09e60_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 4;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %jmp T_1.5;
T_1.0 ;
    %load/vec4 v0x5648b1f09f40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
    %jmp T_1.7;
T_1.6 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
T_1.7 ;
    %jmp T_1.5;
T_1.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
    %jmp T_1.5;
T_1.2 ;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
    %jmp T_1.5;
T_1.3 ;
    %load/vec4 v0x5648b1f09f40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.8, 8;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
    %jmp T_1.9;
T_1.8 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
T_1.9 ;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5648b1f09c70_0, 0, 4;
    %jmp T_1.5;
T_1.5 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5648b1ef4110;
T_2 ;
    %wait E_0x5648b1eeeeb0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5648b1f09b80_0, 0, 2;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5648b1f09aa0_0, 0, 2;
    %load/vec4 v0x5648b1f09e60_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 4;
    %cmp/u;
    %jmp/1 T_2.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_2.4, 6;
    %jmp T_2.5;
T_2.0 ;
    %jmp T_2.5;
T_2.1 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5648b1f09b80_0, 0, 2;
    %jmp T_2.5;
T_2.2 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5648b1f09b80_0, 0, 2;
    %jmp T_2.5;
T_2.3 ;
