S_0x5648b1ef3f90 .scope module, "test_bench" "test_bench" 2 1;
 .timescale 0 0;
v0x5648b1f0a0a0_0 .var "clk", 0 0;
v0x5648b1f0a160_0 .net "cw", 1 0, v0x5648b1f09aa0_0;  1 drivers
v0x5648b1f0a230_0 .net "hw", 1 0, v0x5648b1f09b80_0;  1 drivers
v0x5648b1f0a330_0 .var "reset", 0 0;
v0x5648b1f0a400_0 .var "x", 0 0;
S_0x5648b1ef4110 .scope module, "test" "signal_control" 2 6, 3 2 0, S_0x5648b1ef3f90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "reset"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "x"
    .port_info 3 /OUTPUT 2 "hw"
    .port_info 4 /OUTPUT 2 "cw"
P_0x5648b1eb9030 .param/l "S0" 0 3 8, C4<0000>;
